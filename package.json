{
  "name": "react-starter-kit",
  "version": "0.1.0",
  "private": true,
  "description": "Template project to kickstart new React projects",
  "scripts": {
    "clean": "rm -rf ./build",
    "build": "npm run clean && NODE_ENV=production webpack",
    "start:build": "http-server ./build -p 3001",
    "start": "webpack-dev-server",
    "lint": "concurrently --names eslint,tslint \"eslint -c ./.eslintrc.json ./src/**/*.js\" \"tslint -c ./tslint.json ./src/**/*.ts\"",
    "test": "npm run lint && ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edgejay/react-starter-kit.git"
  },
  "keywords": [
    "react",
    "typescript",
    "styled-components",
    "enzyme",
    "ava"
  ],
  "engines": {
    "node": "10.14.0"
  },
  "author": "Wu Huijie",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/edgejay/react-starter-kit/issues"
  },
  "homepage": "https://github.com/edgejay/react-starter-kit#readme",
  "dependencies": {
    "@types/react": "^16.8.7",
    "@types/react-dom": "^16.8.2",
    "@types/react-redux": "^7.0.3",
    "@types/styled-components": "^4.1.12",
    "classnames": "^2.2.6",
    "dotenv": "^6.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-hot-loader": "^4.8.0",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.1.3",
    "styled-normalize": "^8.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/plugin-transform-spread": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@hot-loader/react-dom": "^16.7.0",
    "@webpack-cli/init": "^0.1.3",
    "ava": "^1.2.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.0",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.0",
    "custom-tslint-formatters": "^2.4.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^13.2.0",
    "node-sass": "^4.11.0",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.1.0",
    "sinon": "^7.2.3",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-node": "^8.0.3",
    "tslint": "^5.13.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "^3.5.4",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3333",
    "typescript-plugin-styled-components": "^1.4.0",
    "webpack": "4.28.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "files": [
      "**/*.test.js",
      "**/*.test.ts"
    ],
    "require": [
      "@babel/register",
      "ts-node/register"
    ]
  }
}
